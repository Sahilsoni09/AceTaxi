{
  "expo": {
    "name": "AceTaxi",
    "slug": "acetaxi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationAlwaysUsageDescription": "This app uses your location in the background to track your movement.",
        "NSLocationWhenInUseUsageDescription": "This app uses your location to track your movement.",
        "UIBackgroundModes": [
          "location"
        ]
      }
    },
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#CD1A21"
      },
      "package": "com.ace.acetaxi",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "4e37a92e-6509-4aa2-ad62-3fffeb456a3a"
      }
    },
    "owner": "soni0903",
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/Fonts/Roboto/Roboto-Regular.ttf"
          ]
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "ace-taxi",
          "organization": "backend-coders-india-dd"
        }
      ]
    ]
  }
}
